!
! Coarse-grained model of photoautotrophic growth
!   


Model
 
 Constants

  NA = 6.022e23 		! Avogadro constant
  f = NA*1.0e-6*1.0e-18*3600.0	! factor to convert light unit from [mu(photons) m-2 s-1] into [molecules(photons) nm-2 h-1]

 End Constants


 Parameters

  ! input parameters
  I = 100.0 			! light intensity
  cix = 1.e5			! external inorganic carbon

  ! fixed parameters
  Vcell = 4.19e-15 		! cell volume (with a cell diameter of 2 mu m)
  Proteins = 1.4e10		! total proteome mass in units of amino acids per cell
  n_et = 1681.0 		! length of bicarbonate transporter, Carboxysomes and CA
  n_em = 28630.0 		! length of metabolic enzyme complex to produce cq
  n_er = 7358.0 		! ribosome length (without RNA molecules)
  n_psu = 95451.0 		! length of photosynthetic units
  n_pq = 300.0 			! average length of quota proteins


  ! stoichiometric coefficients
  mc_em = 5.0			! molecules of ci required to produce one aa
  me_et = 1.0			! energy required for the carbon concentrating mechanism
  me_em = 45.0 			! molecules of e consumed to create one aa
  me_er = 3.0			! molecules of e for one translational elongation step
  m_hv = 0.125			! 8 photons are required to activate one PSU
  m_phi = 8.0 			! molecules of e produced via photosynthesis


  ! variable parameters
  kcat_t = 43560.0 		! maximal import rate of bicarbonate transporter (12.1 s-1)
  Kt = 15.0 			! inorganic carbon import threshold of bicarbonate transporter in muM
  kcat_m = 32700.0 		! maximal carbon fixation rate of RuBisCo (545 min-1)
  Km = 181.0*1.0e-6*NA*Vcell	! carbon fixation threshold of RuBisCo in converted to molecules per cell (181 muM)
  gamma_max = 79200.0 		! maximal translation elongation rate (22 s-1)
  Ka = 1.0e4 			! e and aa threshold
  Ke = 1.0e4 			! e and aa threshold
  dp = 1.0/23.0       		! protein degradation: average protein half-life in bacteria (23 h)
  kme = 7.0e9	 		! maintenance term taken from FBA, where it is 1.3 mmol

  ! amount of quota dry weight and quota proteins 
  PQ = (Proteins*0.5)/n_pq	! amount of quota proteins


  ! estimated parameters
  tau = 100.0*3600.0		! turnover rate of photosystem 
  kd = 1.2e-6			! photodamage rate constant
  sigma = 6.0 			! absorption cross section

 End Parameters


 Variables

  ! species concentrations
  ci = 1.0e8,		>=0.0
  aa = 2.0e6,		>=0.0
  ET = 1.0e4,		>=0.0
  EM = 1.0e4,		>=0.0
  ER = 3.0e4,		>=0.0
  PSU0 = 1.0e4,		>=0.0
  PSU1 = 3.0e4,		>=0.0
  e = 5.0e11,		>=0.0

  ! gene regulatory parameters
  beta_et = 0.1,	>=0.0
  beta_em = 0.02,	>=0.0
  beta_er = 0.01,	>=0.0
  beta_psu = 0.76,	>=0.0
  beta_pq = 0.1,	>=0.0

  growth_rate = 0.1	>=0.0

 End Variables


 Intermediates

  ! nutrient uptake
  vet = ET*kcat_t*(cix/(Kt+cix))*(e/(Ke+e))

  ! carbon metabolism
  vem = EM*kcat_m*(ci/(Km+ci))*(e/(Ke+e))

  ! protein translation
  ver = ER*gamma_max*(e/(Ke+e))*(aa/(Ka+aa))

  ! light harvesting
  v1 = PSU0*sigma*I*f
  v2 = tau*PSU1
  vi = PSU1*kd*I*f

  ! energy maintenance
  vme = kme*e/(10.0+e)

 End Intermediates


 Equations

  1.0 >= beta_et + beta_em + beta_er + beta_psu + beta_pq
  Proteins = ci/mc_em + aa + n_et*ET + n_em*EM + n_er*ER + n_psu*(PSU0+PSU1) + n_pq*PQ

  0 = vet - mc_em*vem - growth_rate*ci
  0 = vem + n_psu*vi + dp*(n_et*ET+n_em*EM+n_er*ER+n_psu*(PSU0+PSU1)+n_pq*PQ) - ver*(beta_et+beta_em+beta_er+beta_psu+beta_pq) - growth_rate*aa
  0 = beta_et*(ver/n_et) - (growth_rate+dp)*ET
  0 = beta_em*(ver/n_em) - (growth_rate+dp)*EM
  0 = beta_er*(ver/n_er) - (growth_rate+dp)*ER
  0 = beta_psu*(ver/n_psu) + v2 - m_hv*v1 - (growth_rate+dp)*PSU0
  0 = m_hv*v1 - v2 - vi - (growth_rate+dp)*PSU1
  0 = beta_pq*(ver/n_pq) - (growth_rate+dp)*PQ
  0 = m_phi*v2 - me_et*vet - me_em*vem - me_er*ver*(beta_et+beta_em+beta_er+beta_psu+beta_pq) - vme - growth_rate*e


  maximize log(growth_rate)

 End Equations

End Model








